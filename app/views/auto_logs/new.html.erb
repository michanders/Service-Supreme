<body class='garage'>
	<%= render 'layouts/header' %><br>
	<div class='entry'><br>
		<p class='vehicleentry'>
			<%= @vehicle.make %>
			<%= @vehicle.model %>
		</p>
		<%= form_for :auto_entry, url: '/entry' do |f| %>
		  <%= f.text_field :entry, placeholder: "Add a service entry", :autocomplete => :off %><br>
		
		  <%= f.hidden_field :vehicle_id, :value => params[:vehicleid] %><br>
		  <%= f.submit :title => "Submit", :type => :image, :src => image_path("/images/login.png") %>
		<% end %>
	</div>
</body>
